{% extends '@Admin/layout.html.twig' %}

{% block page_content %}
    <div class="box">
        <div class="box-header">
            <a href="{{ editUrl }}" class="btn btn-primary">Edit</a>
        </div>
        <div class="box-body">
            <table class="table table-bordered table-striped">
                {% for name, value in data %}
                    <tr>
                        <td width="200"><b>{{ name|normalize }}:</b></td>
                        <td>{{ value }}</td>
                    </tr>
                {% endfor %}
            </table>
        </div>
    </div>
{% endblock %}