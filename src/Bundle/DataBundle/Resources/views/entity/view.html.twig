{% extends '@Data/entity/layout.html.twig' %}

{% block content %}

    <table class="table table-bordered table-striped">
        {{ tag_list(object) }}
        {% for item in data %}
            <tr>
                <td width="200"><b>{{ item.name }}:</b></td>
                <td>{{ item.value }}</td>
            </tr>
        {% endfor %}
    </table>

{% endblock %}